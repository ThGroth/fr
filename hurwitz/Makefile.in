# Makefile for Hurwitz data programs

CXX = g++ -ggdb

all: layout hsolve

layout: layout.C
	$(CXX) @CHOLMODINCLUDE@ @LIBDOGLEGINCLUDE@ $< -o $@ @CHOLMODLIB@ @LIBDOGLEGLIB@ @LDDOGLEG@

hsolve: hsolve.C
	$(CXX) @LEVMARINCLUDE@ $< -o $@ @LEVMARLIB@ @LDLEVMAR@

clean:
	rm -rf hsolve layout *.dSYM Makefile config.h
